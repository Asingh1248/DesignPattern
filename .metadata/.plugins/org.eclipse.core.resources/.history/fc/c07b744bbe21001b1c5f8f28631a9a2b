package com.phone;
//Build my own phone
public class Phone {

   
   public Phone(String os, int ram, String processor, int battery) {
	super();
	this.os = os;
	this.ram = ram;
	this.processor = processor;
	this.battery = battery;
   }

@Override
public String toString() {
	return "Phone [os=" + os + ", ram=" + ram + ", processor=" + processor + ", battery=" + battery + "]";
}
   
   
}
