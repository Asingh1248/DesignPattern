package com.phone;

public class PhoneBuilder {
	
	   private String os;
	   private int ram;
	   private String processor;
	   private int battery;
	   
	 // void replace PhoneBuilder at run time which parameter to set we can get to know  
	public PhoneBuider setOs(String os) {
		this.os = os;
	}
	public PhoneBuider setRam(int ram) {
		this.ram = ram;
	}
	public PhoneBuider setProcessor(String processor) {
		this.processor = processor;
	}
	public PhoneBuider setBattery(int battery) {
		this.battery = battery;
	}
	   
	   
}
